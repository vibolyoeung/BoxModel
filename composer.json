{
	"name": "typo3cms/boxmodel",
	"description": "TYPO3 CMS for Boxmodel website.",
	"keywords": ["boxmodel", "we_default", "we_custom", "typo3", "cms", "web essentials"],
	"homepage": "http://www.web-essentials.asia",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Web Essentials",
			"email": "production@web-essentials.asia",
			"homepage": "http://www.web-essentials.asia"
		}
	],
	"config": {
		"process-timeout": 1800,
		"vendor-dir": "~/.composer"
	},
	"require": {
		"php": ">=5.3.7",
		"composer/installers": "*",
		"typo3cms/we_default": "master",
		"typo3cms/fluidcontent": "4.0.0",
		"typo3cms/fluidcontent_bootstrap": "3.0.0",
		"typo3cms/fluidpages": "3.0.0",
		"typo3cms/flux": "7.0.0",
		"typo3cms/vhs": "1.8.5",
		"typo3cms/realurl": "1.12.8",
	},
	"require-dev": {
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "typo3cms/we_default",
				"version": "master",
				"type": "typo3-cms-extension",
				"source": {
					"url": "git@git.web-essentials.asia:sengchheang/we_default.git",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/fluidcontent",
				"version": "4.0.0",
				"type": "typo3-cms-extension",
				"source": {
					"url": "https://github.com/FluidTYPO3/fluidcontent.git",
					"type": "git",
					"reference": "4.0.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/fluidcontent_bootstrap",
				"version": "3.0.0",
				"type": "typo3-cms-extension",
				"source": {
					"url": "https://github.com/FluidTYPO3/fluidcontent_bootstrap.git",
					"type": "git",
					"reference": "3.0.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/fluidpages",
				"version": "3.0.0",
				"type": "typo3-cms-extension",
				"source": {
					"url": "https://github.com/FluidTYPO3/fluidpages.git",
					"type": "git",
					"reference": "3.0.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/flux",
				"version": "7.0.0",
				"type": "typo3-cms-extension",
				"source": {
					"url": "https://github.com/FluidTYPO3/flux.git",
					"type": "git",
					"reference": "7.0.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/vhs",
				"version": "1.8.5",
				"type": "typo3-cms-extension",
				"source": {
					"url": "https://github.com/FluidTYPO3/vhs.git",
					"type": "git",
					"reference": "1.8.5"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/realurl",
				"version": "1.12.8",
				"type": "typo3-cms-extension",
				"source": {
					"url": "git://git.typo3.org/TYPO3CMS/Extensions/realurl.git",
					"type": "git",
					"reference": "1_12_8"
				}
			}
		}
	],
	"minimum-stability": "dev"
}